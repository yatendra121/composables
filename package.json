{
    "name": "@qnx/composables",
    "description": "Collection of basic Vue Composition Utilities",
    "version": "0.6.22",
    "type": "module",
    "license": "MIT",
    "author": "Yatendra Kushwaha <yatendra121kushwaha@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/yatendra121/composables.git"
    },
    "funding": "https://github.com/sponsors/yatendra121",
    "scripts": {
        "serve": "vite preview",
        "build": "vite build && vue-tsc --pretty  --declaration --emitDeclarationOnly",
        "dev": "vite",
        "build:types": "rollup --config build/rollup.types.config.js",
        "build-force": "pnpm build && pnpm build:types",
        "test:unit": "vitest"
    },
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/yatendra121/composables/issues"
    },
    "keywords": [
        "composables",
        "utils",
        "vue3"
    ],
    "sideEffects": false,
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./*": "./*",
        "./axios": {
            "types": "./src/axios.ts",
            "import": "./src/axios.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "src/index.ts"
            ],
            "axios": [
                "src/axios.ts"
            ]
        }
    },
    "publishConfig": {
        "main": "./dist/index.js",
        "module": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "require": "./dist/index.js"
            },
            "./*": "./*",
            "./axios": {
                "types": "./dist/axios.d.ts",
                "require": "./dist/axios.js",
                "import": "./dist/axios.js"
            }
        },
        "typesVersions": {
            "*": {
                "*": [
                    "dist/index.d.ts"
                ],
                "axios": [
                    "dist/axios.d.ts"
                ]
            }
        }
    },
    "devDependencies": {
        "@types/micromatch": "^4.0.9",
        "@vitejs/plugin-vue": "^6.0.0",
        "axios": "^1.10.0",
        "fast-glob": "^3.3.3",
        "jsdom": "^26.1.0",
        "micromatch": "^4.0.8",
        "path": "^0.12.7",
        "rollup": "^4.44.1",
        "rollup-plugin-dts": "^6.2.1",
        "tslib": "^2.8.1",
        "typescript": "5.8.3",
        "vite": "^7.0.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4",
        "vue": "3.5.17",
        "vue-tsc": "^2.2.10"
    },
    "peerDependencies": {
        "axios": "*",
        "vue": "*"
    },
    "peerDependenciesMeta": {
        "axios": {
            "optional": true
        },
        "vue": {
            "optional": true
        }
    }
}
